{
  "diagnostic.enable": false,
  "jedi": {
    "enable": true,
    "startupMessage": true,
    "executable.command": "~/.pyenv/versions/neovim3/bin/jedi-language-server"
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
